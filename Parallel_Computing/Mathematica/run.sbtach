#!/bin/bash
#SBATCH -J parallel_pi
#SBATCH -o parallel_pi.out
#SBATCH -e parallel_pi.err
#SBATCH -p shared
#SBATCH -N 1
#SBATCH -c 8
#SBATCH -t 0-00:30
